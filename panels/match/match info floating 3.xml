			</record>
		</container>
		
	    <widget class="preselected_match_chalkboard_panel" pitch_draw_arrows="true" vertical="true" pitch_dont_draw_indicators="false" force_left_to_right_events="true" id="pCak" team_to_display="1" average_position_filter="1" enable_popup="ture">
